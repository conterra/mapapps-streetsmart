    "bundles": {
        "dn_streetsmart": {
            "Config": {
                "streetSmartProperties": {
                    "apiKey": "",
                    "username": "",
                    "password": "",
                    "srs": "EPSG:25832",
                    "locale": "de",
                    "overlayDrawDistance": 30,
                    "addressSettings": {
                        "locale": "de",
                        "database": "Nokia"
                    },
                    "panoramaViewerInstance": {
                        "method": "lookAtCoordinate",
                        "parameters": {
                            "coordinate": [
                                359820,
                                5647699
                            ]
                        }
                    }
                },
                "streetSmartLayerId": "test",
                "useMapCenterLocation": true,
                "closeOnNoData": false,
                "mapCenterLocationDelay": 500
            }
