                "image": "resource('${app}:/assets/logo.svg')",
                "imageWidth": 150,
                "imagePosition": "above",
                "link": "http://conterra.github.io"
            }
        },
        "basemaptoggler": {
            "Config": {
                "itemStyles": {
                    "gray": "icon-thumbnail",
                    "streets": "icon-thumbnail",
                    "satellite": "icon-thumbnail",
                    "hybrid": "icon-thumbnail"
                }
            }
        },
